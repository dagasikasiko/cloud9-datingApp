<h1 class="heading">ユーザー一覧</h1>
<!--%= link_to '新規ユーザー作成', new_user_path, class: "btn add-user-btn" %>-->
<%= link_to 'プロフィールを編集', edit_user_path(current_user),class: "btn add-user-btn" %>

<ul class="user-list"><!--sers[0]-->
  <%= @users.each do |user| %>
    <li>
      <%= image_for(user) %>
      <%= link_to user.name, user %> |
      <%= user.email %>
    </li>
  <% end %>
</ul>
