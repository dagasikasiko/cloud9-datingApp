<p id="notice"><%= notice %></p>

<h1>Userimages</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userimages.each do |userimage| %>
      <tr>
        <td><%= userimage.user_id %></td>
        <td><%= link_to 'Show', userimage %></td>
        <td><%= link_to 'Edit', edit_userimage_path(userimage) %></td>
        <td><%= link_to 'Destroy', userimage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userimage', new_userimage_path %>
