<h1>投稿の一覧[管理]</h1>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>No.</th>
      <th>User</th>
      <th>タイトル</th>
      <th>本文</th>
      <th>投稿日時</th>
      <th>更新日時</th>
      <th></th>
    </tr>
  </thead>
  <% @note.each do |note| %>
    <%= render note %>
  <% end %>
</table>
